{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "npm run inject-theme && svelte-kit sync && vite dev",
    "build": "npm run inject-theme && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "inject-theme": "node scripts/inject-theme.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.3.1",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.873.0",
    "@aws-sdk/s3-request-presigner": "^3.873.0"
  }
}
